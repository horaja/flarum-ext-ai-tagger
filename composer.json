{
	"name": "horaja/flarum-ext-ai-tagger",
	"description": "Automatically suggests tags for Flarum discussions using AI.",
	"type": "flarum-extension",
	"require": {
			"flarum/core": "^1.8.0",
			"flarum/tags": "*",
			"guzzlehttp/guzzle": "^7.0",
			"blomstra/database-queue": "*"
	},
	"autoload": {
			"psr-4": {"Horaja\\AiTagger\\": "src/"}
	},
	"extra": {
			"flarum-extension": {
					"title": "AI Tagger",
					"icon": {
							"name": "fas fa-tags",
							"backgroundColor": "#4285F4",
							"color": "#fff"
					}
			}
	},
	"license": "MIT",
	"author": [
		{
			"name": "Husain Raja",
			"email": "horaja@andrew.cmu.edu"
		}
	]
}